<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="boolean" name="autoTint"/>
    <attr format="integer" name="cutCount"/>
    <attr format="dimension" name="cutHeight"/>
    <attr format="enum" name="cutType">
        <enum name="linePositive" value="0"/>
        <enum name="lineNegative" value="1"/>
        <enum name="arc" value="2"/>
        <enum name="wave" value="3"/>
    </attr>
    <attr format="dimension" name="lineSpacing"/>
    <attr format="float" name="openFactor"/>
    <attr format="reference" name="src"/>
    <attr format="string" name="subTitleText"/>
    <attr format="color" name="subTitleTextColor"/>
    <attr format="dimension" name="subTitleTextSize"/>
    <attr format="dimension" name="textMargin"/>
    <attr format="integer" name="tintAlpha"/>
    <attr format="color" name="tintColor"/>
    <attr format="string" name="titleText"/>
    <attr format="color" name="titleTextColor"/>
    <attr format="dimension" name="titleTextSize"/>
    <declare-styleable name="GlazyImageView"><attr name="src"/><attr name="cutType"/><attr name="cutHeight"/><attr name="cutCount"/><attr name="autoTint"/><attr name="tintColor"/><attr name="tintAlpha"/><attr name="titleText"/><attr name="titleTextColor"/><attr name="titleTextSize"/><attr name="subTitleText"/><attr name="subTitleTextColor"/><attr name="subTitleTextSize"/><attr name="textMargin"/><attr name="lineSpacing"/><attr name="openFactor"/></declare-styleable>
    <declare-styleable name="GlazyViewPager"/>
    <string name="app_name">Glazy</string>
</resources>